<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../XSD/PSD_IFC4.xsd">
	<IfcVersion version="IFC4"/>
	<Name>Pset_DistributionPortPHistoryPipe</Name>
